<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">
    <changeSet id="0002" author="ksmardz">
        <createTable tableName="FOOD_TYPES">
            <column name="food_id" type="java.lang.Long" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="food_type" type="java.lang.String">
                <constraints nullable="false"/>
            </column>
            <column name="RESTAURANT_ID" type="java.lang.Long">
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
